.box-tabs{
	&:after{
		display: block;
		position: absolute;
		height: 3px;
		background: #1d94ff;
		bottom: 0;
		left: 0;
		right: 0;
		content: '';
		}
}

.responsive-tabs__list{
	.clearfix();
}
