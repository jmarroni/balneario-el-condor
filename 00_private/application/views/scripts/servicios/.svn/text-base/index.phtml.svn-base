    <script> 

    $( document ).ready(function() {

        /*############# UPLOAD DE ARCHIVOS x5 #############*/
           /* $("#fileuploader").uploadFile({
                url:"/clasificados/upload",
                fileName:"myfile",
                maxFileCount:1,
                showDownload:false,
                onSuccess:function(files,data,xhr,pd)
                {
                    $("#uploadLogo").val(data);
                }
            });*/

            var contador= '';

            $("#fileuploaders").uploadFile({
                url:"/servicios/upload",
                fileName:"myfile",
                maxFileCount:1,
                multiple: true,
                showDownload:false,
                onSuccess:function(files,data,xhr,pd)
                {   
                    if(data!=''){
                    $("#uploadLogos"+contador).val(data); 
                    contador++;   
                }

                    
                    /*$("#upload-1").css("display", "inline");*/
                }
            });
               

      /*###################### FIN UPLOAD #############################*/

      $("#subir").click(function(event){
        event.preventDefault();
        if( $('#tituloForm').val()!='' && $('#dirForm').val()!='' && $('#correoForm').val()!=''){
          var dataString = 'titulo='+ $('#tituloForm').val() + 
                            '&descripcion=' + $('#descripcionForm').val() + 
                            '&direccion=' + $('#dirForm').val() + 
                            '&nombre=' + $('#nomForm').val() + 
                            '&correo=' + $('#correoForm').val() + 
                            "&archivo=" + $('#uploadLogos').val();
          
          $.ajax({
              url: "/servicios/cargarservicio", // Url to which the request is send
              type: "POST",             // Type of request to be send, called as method
              data:  dataString,// Data sent to server, a set of key/value pairs (i.e. form fields and values)
              success: function(data)   // A function to be called if request succeeds
              {

                  alert('El servicio se ha subido correctamente.');
                  document.location.href = "/servicios/detalle/id/"+data;
              }
          });
        } else {
          alert ('Verifique que todos los datos estan completos');
        }
      });
});
