    <script type="text/javascript">
      $('#consultar').click(function(event) {
        $('#zonaForm').val(1);
      });
      $('#publicitar').click(function(event) {
        event.preventDefault();
        if( $('#nombreForm').val()!='' && $('#emailForm').val()!='' && $('#comentarioForm').val()!=''){
          var dataString = 'nombre=' + $('#nombreForm').val() + '&apellido=' + $('#apellidoForm').val() + '&email=' + $('#emailForm').val() + '&zona=' + $('#zonaForm').val() + '&comentario=' + $('#comentarioForm').val();

          
          $.ajax({
              url: "/index/consultarpublicidad", // Url to which the request is send
              type: "POST",             // Type of request to be send, called as method
              data:  dataString,// Data sent to server, a set of key/value pairs (i.e. form fields and values)
              success: function(data)   // A function to be called if request succeeds
              {

                  alert('La consulta se envio correctamente. En breve nos contactaremos con usted.');
                  $('#formClasificado').hide();
              }
          });
        } else {
          alert ('Verifique que todos los datos esten completos');
        }    
      });
   </script>
